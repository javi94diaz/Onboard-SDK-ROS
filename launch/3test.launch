<!-- Testing namespace (Test apart the respawn="true") (Test apart the  launch-prefix="bash -c 'sleep 10.0; $0 $@' ")-->
<launch>
    <group ns="UAVDJI_MATRICE1">
        <arg name="msg" default="LAUNCHED FROM 3test" />
        <node name="dji_pub_text" pkg="rostopic" type="rostopic"
            args="pub /msg std_msgs/String '$(arg msg)'"
            output="screen" />
        <node name="dji_print_text" pkg="rostopic" type="rostopic"
            args="echo /msg" output="screen" />
        <node pkg="dji_osdk_ros" type="dji_vehicle_node" name="vehicle_node" output="screen">
            <param name="acm_name" type="string" value="/dev/ttyACM0"/>
            <param name="serial_name" type="string" value="/dev/ttyUSB0"/>
            <param name="baud_rate" type="int" value="230400"/>
            <param name="app_id" type="int" value="1129522"/>
            <param name="app_version" type="int" value="1"/>
            <param name="align_time" type="bool" value="false"/>
            <param name="enc_key" type="string" value="5b15be1da21c6a882ddb2aa8ce9eef7f270d65ff5fd99f225d714e04e39e1110"/>
            <param name="use_broadcast" type="bool" value="false"/>
        </node>
    </group>
</launch>